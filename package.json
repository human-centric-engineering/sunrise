{
  "name": "sunrise",
  "version": "1.0.0",
  "description": "A production-ready Next.js starter template designed for rapid application development with AI assistance.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/human-centric-engineering/sunrise.git"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "react",
    "starter",
    "template",
    "boilerplate",
    "prisma",
    "tailwind",
    "nextauth"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/human-centric-engineering/sunrise/issues"
  },
  "homepage": "https://github.com/human-centric-engineering/sunrise#readme",
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
